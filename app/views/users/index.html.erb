<% provide(:title, 'All users') %>
<% content_for :content do %>
  <h1>
    All Users</h1>
  <div class="row">
    <div id="user_switch" class="span4 offset9">
      <button class="btn btn-primary" type="button">Switch to other users</button>
    </div>
  </div>
  <div>
    <%= render @users %>
  </div>
  <%= will_paginate @users %>
<% end %>